Algoritmo sin_titulo
	Definir  n Como Entero;
 	Definir  c Como entero;
    definir sum Como Real;
	c<-1;
	sum<-0;
	mientras c<=5 Hacer
		Escribir "ingrese numero " ,c, "  : ";
		Leer n;
		sum<-sum+n;
		c<-c+1;
	FinMientras
	Escribir " la suma es " , sum;
	Escribir " el promedio es " , sum /5;
	
FinAlgoritmo
